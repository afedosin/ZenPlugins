<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen>
    <EditTextPreference
        key="inn"
        title="ИНН компании"
        dialogTitle="ИНН компании"
        dialogMessage="Введите ИНН компании"
        summary="|ИНН компании|{@s}"
        positiveButtonText="ОК"
        negativeButtonText="Отмена"
        obligatory="true">
    </EditTextPreference>
    <EditTextPreference
        key="startDate"
        obligatory="true"
        inputType="date"
        title="С какой даты загружать операции"
        defaultValue="2018-04-01T00:00:00.000Z"
        dialogTitle="Дата начала загрузки"
        dialogMessage="Введите дату, с которой загружать операции"
        positiveButtonText="ОК"
        negativeButtonText="Отмена"
        summary="|startDate|{@s}">
    </EditTextPreference>
</PreferenceScreen>
